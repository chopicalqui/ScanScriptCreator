[NmapScripts]
tcp = http-title,nfs-showmount,http-methods,smb-enum-*,vnc-info,krb5-enum-users,vnc-title,http-robots.txt,x11-access,ms-sql-info,pop3-capabilities,ssl-cert,ftp-anon,imap-capabilities,smtp-commands,rmi-dumpregistry,mongodb-info,mongodb-databases
udp = ms-sql-info,x11-access,dns-service-discovery,ntp-info

[NmapRttComputation]
factor_init = 4
factor_max = 10
max_scan_delay = 150ms

[NmapGeneralSettings]
default_options = -v --stats-every 10 --reason -Pn -sV --max-retries 1 --min-hostgroup 64 --traceroute
default_options_first_run = -O
default_options_not_first_run = -n
tcp_options = -sS --defeat-rst-ratelimit
udp_options = -sU --defeat-icmp-ratelimit
exec_windows = C:\Program Files (x86)\nmap\nmap.exe
exec_linux = /usr/bin/nmap

[MasscanGeneralSettings]
default_options = --open
exec_linux = /usr/bin/masscan
